{
  "name": "bbr-api",
  "version": "0.0.1",
  "description": "",
  "author": "",
  "private": true,
  "license": "UNLICENSED",
  "scripts": {
    "build": "nest build",
    "format": "prettier --write \"src/**/*.ts\" \"test/**/*.ts\"",
    "start": "nest start",
    "start:dev": "dotenv -e .env.development.local -- nest start --watch",
    "start:debug": "dotenv -e .env.development.local -- nest start --debug --watch",
    "start:prod": "node dist/src/main",
    "typeorm": "typeorm-ts-node-commonjs",
    "migration:typeorm:run": "dotenv -e .env.development.local -- npm run typeorm -- migration:run -d dist/shared/infrastructure/database/data-source.js",
    "migration:knex:run": "dotenv -e .env.development.local pnpm knex migrate:latest --knexfile knexfile.ts",
    "migration:knex:rollback": "dotenv -e .env.development.local pnpm knex migrate:rollback --knexfile knexfile.ts",
    "migration:revert": "dotenv -e .env.development.local -- npm run typeorm -- migration:revert -d dist/shared/infrastructure/database/data-source.js",
    "migration:create": "dotenv -e .env.development.local -- npm run typeorm -- migration:create db/migrations/${npm_config_name}",
    "seed:knex:run:all": "dotenv -e .env.development.local pnpm knex seed:run --knexfile knexfile.ts",
    "seed:knex:run": "dotenv -e .env.development.local pnpm ts-node seed-run.ts",
    "lint": "eslint \"{src,apps,libs,test}/**/*.ts\" --fix",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:cov": "jest --coverage",
    "test:debug": "node --inspect-brk -r tsconfig-paths/register -r ts-node/register node_modules/.bin/jest --runInBand",
    "test:e2e": "jest --config ./test/jest-e2e.json"
  },
  "dependencies": {
    "@aws-sdk/client-s3": "^3.758.0",
    "@aws-sdk/s3-request-presigner": "^3.758.0",
    "@nestjs-modules/mailer": "^2.0.2",
    "@nestjs/bullmq": "^11.0.2",
    "@nestjs/common": "^11.0.11",
    "@nestjs/config": "^4.0.1",
    "@nestjs/core": "^11.0.1",
    "@nestjs/event-emitter": "^3.0.1",
    "@nestjs/mongoose": "^11.0.3",
    "@nestjs/passport": "^11.0.5",
    "@nestjs/platform-express": "^11.0.1",
    "@nestjs/schedule": "^5.0.1",
    "@nestjs/swagger": "^11.0.6",
    "@nestjs/typeorm": "^11.0.0",
    "aws-sdk": "^2.1692.0",
    "bcrypt": "^5.1.1",
    "bullmq": "^5.44.0",
    "class-transformer": "^0.5.1",
    "class-validator": "^0.14.1",
    "connect-redis": "^8.0.2",
    "csv-parser": "^3.2.0",
    "dotenv": "^16.4.7",
    "express-session": "^1.18.1",
    "handlebars": "^4.7.8",
    "ioredis": "^5.6.0",
    "knex": "^3.1.0",
    "mime-types": "^2.1.35",
    "mongoose": "^8.15.1",
    "mustache": "^4.2.0",
    "nodemailer": "^6.10.0",
    "objection": "^3.1.5",
    "openai": "^5.0.2",
    "passport": "^0.7.0",
    "passport-google-oauth20": "^2.0.0",
    "passport-local": "^1.0.0",
    "pg": "^8.13.3",
    "reflect-metadata": "^0.2.2",
    "rxjs": "^7.8.1",
    "stripe": "^18.1.0",
    "swagger-ui-express": "^5.0.1",
    "uuid": "^11.1.0"
  },
  "devDependencies": {
    "@eslint/eslintrc": "^3.2.0",
    "@eslint/js": "^9.18.0",
    "@nestjs/cli": "^11.0.0",
    "@nestjs/schematics": "^11.0.0",
    "@nestjs/testing": "^11.0.1",
    "@swc/cli": "^0.6.0",
    "@swc/core": "^1.10.7",
    "@types/bcrypt": "^5.0.2",
    "@types/express": "^5.0.0",
    "@types/express-session": "^1.18.1",
    "@types/jest": "^29.5.14",
    "@types/multer": "^1.4.12",
    "@types/mustache": "^4.2.6",
    "@types/node": "^22.10.7",
    "@types/passport": "^1.0.17",
    "@types/supertest": "^6.0.2",
    "dotenv-cli": "^8.0.0",
    "eslint": "^9.18.0",
    "eslint-config-prettier": "^10.1.1",
    "eslint-plugin-prettier": "^5.2.3",
    "globals": "^16.0.0",
    "inquirer": "^12.5.2",
    "jest": "^29.7.0",
    "lodash": "^4.17.21",
    "prettier": "^3.5.3",
    "source-map-support": "^0.5.21",
    "supertest": "^7.0.0",
    "ts-jest": "^29.2.5",
    "ts-loader": "^9.5.2",
    "ts-node": "^10.9.2",
    "tsconfig-paths": "^4.2.0",
    "typeorm": "^0.3.21",
    "typescript": "^5.7.3",
    "typescript-eslint": "^8.20.0"
  },
  "jest": {
    "moduleFileExtensions": [
      "js",
      "json",
      "ts"
    ],
    "rootDir": "src",
    "testRegex": ".*\\.spec\\.ts$",
    "transform": {
      "^.+\\.(t|j)s$": "ts-jest"
    },
    "collectCoverageFrom": [
      "**/*.(t|j)s"
    ],
    "coverageDirectory": "../coverage",
    "testEnvironment": "node"
  }
}
